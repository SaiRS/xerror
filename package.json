{
  "name": "xerror",
  "version": "1.0.0",
  "description": "custom error",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "generate": "plop --plopfile generators/index.js",
    "lint-staged": {
      "src/**/*.{js,vue,jsx,json,ts,tsx}": [
        "prettier --config .prettierrc --write",
        "eslint",
        "git add"
      ],
      "src/**/*.{css,scss,less}": [
        "prettier --config .prettierrc --write",
        "stylelint --syntax=scss",
        "git add"
      ]
    },
    "format": "prettier --write '**/*.{js,jsx,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaiRS/xerror.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SaiRS/xerror/issues"
  },
  "homepage": "https://github.com/SaiRS/xerror#readme",
  "dependencies": {
    "ts-node": "^8.0.1",
    "express": "^4.16.4",
    "cookie-parser": "^1.4.3",
    "morgan": "^1.9.1",
    "superagent": "^4.1.0",
    "http-errors": "^1.7.1",
    "mongoose": "^5.4.9",
    "mongodb": "^3.1.13",
    "faker": "^4.1.0",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-compat": "^2.6.3",
    "shelljs": "^0.8.3",
    "jest": "^24.0.0",
    "prettier": "^1.16.0",
    "pretty-quick": "^1.6.0",
    "eslint": "^5.12.1",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "eslint-config-alloy": "^1.4.2",
    "@types/jest": "^24.0.0",
    "ts-jest": "^23.10.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
